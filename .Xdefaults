#URxvt*highlightColor:   magenta


xterm*scrollBar: false
xterm*vt100.scrollBar: false
xterm*vt100.font:		-*-lucida-*-*-*-*-*-*-*-*-*-*-*-*
xterm*background: black
xterm*foreground: white
xterm*multiScroll: on
xterm*jumpScroll: on
xterm*SaveLines: 10000



Terminator*transparent:true
Terminator*scrollBar:false
Terminator*geometry:124x24    #I don't use the whole screen, if you want a full screen term don't bother with this and see below.
Terminator*borderLess:true
Terminator*foreground:Black   #Font color. My wallpaper is White, you may wish to change this to White.


